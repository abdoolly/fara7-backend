{
  "name": "remote-teacher",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./build/bin/www",
    "prod": "PORT=80 node ./build/bin/www",
    "dev": "nodemon ./build/bin/www",
    "watch": "tsc --watch",
    "build": "tsc",
    "docker": "docker-compose up -d",
    "prisma:stop": "docker-compose stop",
    "migrate": "prisma migrate save --name init --experimental && prisma migrate up --experimental && prisma generate"
  },
  "dependencies": {
    "@prisma/cli": "^2.0.0-beta.2",
    "@prisma/client": "^2.0.0-beta.2",
    "apollo-server": "^2.11.0",
    "apollo-server-express": "^2.11.0",
    "casual": "^1.6.2",
    "class-validator": "^0.12.1",
    "cookie-parser": "~1.4.4",
    "dateformat": "^3.0.3",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-jwt": "^5.3.1",
    "graphql": "^14.6.0",
    "graphql-scalars": "^1.0.9",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "morgan": "~1.9.1",
    "ramda": "^0.27.0"
  },
  "devDependencies": {
    "@types/dateformat": "^3.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.3",
    "@types/express-jwt": "^0.0.42",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.9.0",
    "@types/node": "^12.12.31",
    "@types/passport-jwt": "^3.0.3",
    "@types/ramda": "^0.27.0",
    "nodemon": "^2.0.2",
    "typescript": "^3.8.3"
  }
}